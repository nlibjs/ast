{
	"name": "@nlib/ast",
	"publishConfig": {
		"access": "public"
	},
	"version": "0.3.1",
	"description": "Toolset for manipulating AST.",
	"author": {
		"name": "Kei Ito",
		"email": "kei.itof@gmail.com",
		"url": "https://github.com/kei-ito"
	},
	"license": "MIT",
	"engines": {
		"node": ">=8"
	},
	"repository": "nlibjs/ast",
	"main": "src/index.js",
	"scripts": {
		"lint": "eslint --no-inline-config src test",
		"test": "tap --coverage test/comment test/generator test/walker"
	},
	"devDependencies": {
		"@nlib/lint": "^3.3.3",
		"acorn": "^6.1.0",
		"acorn-walk": "^6.1.1",
		"eslint": "^5.13.0",
		"tap": "^12.5.2"
	},
	"eslintConfig": {
		"extends": "./node_modules/@nlib/lint/.eslintrc.json",
		"env": {
			"es6": true,
			"node": true
		},
		"rules": {
			"func-style": "off",
			"max-lines-per-function": "off",
			"no-use-before-define": "off"
		}
	}
}
